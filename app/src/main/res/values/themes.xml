<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AndroidFinal" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#00818a</item>
        <item name="colorPrimaryVariant">#09194f</item>
        <item name="colorOnPrimary">#fdfdeb</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#f9ce00</item>
        <item name="colorSecondaryVariant">#00818a</item>
        <item name="colorOnSecondary">#09194f</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.AndroidFinal.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.AndroidFinal.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.AndroidFinal.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>